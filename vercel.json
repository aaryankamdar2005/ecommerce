{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json", 
        "use": "@vercel/static-build",  
        "config": {
          "distDir": "build" 
        }
      },
      {
        "src": "routes/**/*.js",  
        "use": "@vercel/node"     
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/build/$1"  
      },
      {
        "src": "/api/(.*)",  
        "dest": "/routes/$1.js"  
      }
    ]
  }
  